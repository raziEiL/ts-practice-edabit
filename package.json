{
    "name": "js-practice-edabit",
    "version": "1.0.0",
    "description": "Solved exercises in Javascript from Edabit.",
    "author": "Denis Medvedev (raziEiL)",
    "homepage": "https://github.com/raziEiL",
    "private": true,
    "devDependencies": {
        "@types/jest": "^25.1.4",
        "@types/node": "^13.9.0",
        "jest": "^24.9.0",
        "ts-jest": "^25.2.1",
        "typescript": "^3.8.3"
    },
    "jest": {
        "roots": ["<rootDir>/src"],
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
        "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
    },
    "scripts": {
        "test": "jest --no-cache ./*",
        "watch": "jest --no-cache --watch ./*"
    },
    "license": "MIT",
    "dependencies": {}
}